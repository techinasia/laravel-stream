{
    "name": "techinasia/laravel-stream",
    "description": "Stream bridge for Laravel 5.",
    "homepage": "https://github.com/techinasia/laravel-stream",
    "keywords": ["laravel", "stream"],
    "license": "MIT",
    "authors": [
        {
            "name": "Melvin Lee",
            "email": "melvin@techinasia.com",
            "homepage": "http://zy.sg",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "^5.1|^5.2|^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "get-stream/stream": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Techinasia\\GetStream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Techinasia\\GetStream\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
